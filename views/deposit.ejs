            <div class="row">
	<div class="col-md-8">
		<div class="card">
			<div class="card-header">
				<h4 class="mb-0"><i class="fas fa-credit-card me-2"></i>Deposit Baru</h4>
			</div>
			<div class="card-body pb-3">
				<form method="post" id="depositForm">
                    <input type="hidden" name="csrf_token" value="fe3dbad0837dc899e31cfd3731611d27" />
                    <div class="form-group">
                        <label class="form-label">Jenis Pembayaran <span class="text-danger">*</span></label>
                        <select class="select2 form-control" name="type" id="type" disabled>
                            <option value="">QRIS OTOMATIS</option>
                        </select>
                    </div>
                    <div class="form-group" hidden>
                        <label class="form-label">Email</label>
                        <div class="input-group">
                            <input type="text" class="form-control" name="email" id="email" value="<%= email %>" disabled>
                        </div>
                    </div>
                    <div class="form-group" hidden>
                        <label class="form-label">Username</label>
                        <div class="input-group">
                            <input type="text" class="form-control" name="username" id="username" value="<%= username %>" disabled>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="form-label">Jumlah Deposit</label>
                        <div class="input-group">
                            <div class="input-group-text">Rp.</div>
                            <input type="number" class="form-control" name="amount" id="depositAmount" value="1000">
                        </div>
                    </div>
                    <hr>
                    <div class="hstack gap-1 justify-content-end">
                        <button type="button" id="submitBtn" class="btn btn-primary" onclick="submitForm()">
                            <i class="fas fa-credit-card fs-6 me-2"></i>Deposit
                        </button>
                    </div>
                </form>
                
                
			</div>
		</div>
	</div>
    
    <div class="col-md-4">
        <div class="card-body">
            <div id="resultCard" class="card d-none">
