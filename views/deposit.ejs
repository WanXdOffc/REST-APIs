                    </div>
                    <hr>
                    <div class="mb-2">
                        <strong>Status:</strong> <span id="resultStatus"></span>
                    </div>
                    <hr>
                    <div class="mb-2">
                        <strong>Payment:</strong> <span id="resultPayment"></span>
                    </div>
                    <hr>
                    <form action="/users/deposit/check" method="GET">
                        <input class="form-control" type="hidden" name="id" id="id" value="" />
                        <button id="" class="btn btn-primary" type="submit" onclick="setHiddenFieldValue()"><i class="fa fa-exchange" aria-hidden="true"></i> Konfirmasi Pembayaran</button>
                    </form>
                
                    <script>
                        function setHiddenFieldValue() {
                            // Set the value of the hidden input field with the ID "id" to the value of "resultNoRef"
                            document.getElementById("id").value = document.getElementById("resultNoRef").innerText;
                        }
                    </script>
                </div>
            </div>
        </div>
