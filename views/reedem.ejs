  <!-- Cek Apikey -->
 <div class="row">
	<div class="col-md-6">
		<div class="card">
			<div class="card-header">
				<h4 class="mb-0"><i class="fas fa-credit-card me-2"></i>Deposit Baru</h4>
			</div>
			<div class="card-body pb-3">
					<div class="form-group">
                <form action="/users/redeem-code" method="POST">
                    <div class="form-group">
                        <label class="form-label">Code <span class="text-danger">*</span></label>
                        <input type="text"id="code" name="code" class="form-control" placeholder="HC-XXXXXXXX" value="HC-XXXXX" required>
                        <input type="hidden" id="email" name="email" value="<%= email %>">
                    </div>
                    <div class="mb-0">
                        <button type="submit" class="btn btn-primary float-end"><i class="fa fa-code fs-6 me-2"></i>Submit</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
<div class="col-md-5">
    <div class="card">
        <div class="card-header py-3">
            <h4 class="mb-0"><i class="fas fa-info-circle me-2"></i>Informasi</h4>
        </div>
        <div class="card-body">
            <form onsubmit="event.preventDefault(); execute();">
                <div class="form-group">
                    <label class="form-label">Apikey <span class="text-danger">*</span></label>
                    <input type="text" id="input-apikey" class="form-control" name="apikey" placeholder="Masukan apikey" value="" required>
                </div>
                <div class="mb-0">
                    <button type="submit" id="submit-apikey" class="btn btn-primary float-end"><i class="fa fa-key fs-6 me-2"></i>Submit</button>
                </div>
            </form>
        </div>
    </div>
</div>
