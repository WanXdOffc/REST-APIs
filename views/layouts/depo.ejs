    <script>
        if (!($(".modal").data('bs.modal') || {})._isShown) {
            document.querySelector('body').style.overflow = 'auto';
            $('.select2').select2();
        }
        function submitForm() {
        // Menyembunyikan tombol setelah diklik
        document.getElementById("submitBtn").style.display = "none";

        // Mendapatkan nilai jumlah deposit
        var amountValue = document.getElementById("depositAmount").value;

        // Validasi jumlah deposit
        if (amountValue < 1000 || amountValue > 100000) {
            Swal.fire({
                icon: 'error',
                title: 'Error',
                text: 'Jumlah deposit minimal 1k sampai 100k',
            });
            // Menampilkan kembali tombol setelah validasi gagal
            document.getElementById("submitBtn").style.display = "block";
            return; // Menghentikan eksekusi fungsi jika validasi gagal
        }

        // Set action form dan kirim permintaan seperti sebelumnya
        document.getElementById("depositForm").action = "/users/deposit?username=<%= username %>&email=<%= email %>&amount=" + amountValue;

        fetch(document.getElementById("depositForm").action, {
            method: 'POST',
            body: new FormData(document.getElementById("depositForm")),
        })
        .then(response => response.json())
        .then(data => {
            updateResultCard(data);
            showResultCard();
        })
        .catch(error => {
            console.error('Error:', error);
        })
        .finally(() => {
            document.getElementById("submitBtn").disabled = false;
        });
    }

    // Set the innerText of the elemen
        function updateResultCard(data) {
            var expirationTimestamp = data.expired;
            var expirationDate = new Date(expirationTimestamp * 1000); // Convert seconds to milliseconds

            // Format the time as "jam:menit:detik"
            var options = { hour: 'numeric', minute: 'numeric', second: 'numeric' };
            var formattedExpirationTime = expirationDate.toLocaleTimeString('id-ID', options);
            var formattedExpirationDate = expirationDate.toLocaleDateString('id-ID', options);
            document.getElementById("resultEmail").innerText = data.email || "N/A";
            document.getElementById("resultUsername").innerText = data.username || "N/A";
            document.getElementById("resultNoRef").innerText = data.no_ref || "N/A";
            document.getElementById("resultHarga").innerText = data.harga || "N/A";
            document.getElementById("resultFee").innerText = data.fee || "N/A";
            document.getElementById("resultTotalHarga").innerText = data.total_harga || "N/A";
            document.getElementById("resultStatus").innerText = data.status || "N/A";
            document.getElementById("resultPayment").innerText = data.payment || "N/A";
            document.getElementById("resultExpired").innerText = formattedExpirationDate || "N/A";
            document.getElementById("resultQrUrl").innerText = data.qr_url || "N/A";
            document.getElementById("resultQrUrl").src = data.qr_url || "#"; // Set a default value or remove the link if not available
        }
    
        function showResultCard() {
            document.getElementById("resultCard").classList.remove("d-none");
        }
    </script>
