    <script type="text/javascript">
        // Fetch data from the API
        fetch('https://api.waifu.pics/sfw/waifu')
            .then(response => response.json())
            .then(data => {
                // Assuming this code is within an HTML file or will be used in the browser
                // Replace the background property with the correct style
                document.querySelector('.info-side').style.background = `url('${data.url}') no-repeat center center / cover`;
            })
            .catch(error => console.log('Error fetching data:', error));
    </script>
 <script>
    fetch('https://rzky.my.id/api/anime/quotesanime')
        .then(response => response.json())
        .then(data => {
            const quoteElement = document.getElementById('quote');
            // Assuming you want the quote from the first item in the result array
            const firstQuote = data.result[0].quotes;
            quoteElement.textContent = firstQuote;
        })
        .catch(error => console.error('Error fetching quote:', error));
</script>
<script>
