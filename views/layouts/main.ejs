<script>
    function execute() {
        // Mendapatkan nilai apikey dari input
        var apiKeyValue = document.getElementById("input-apikey").value;

        // Melakukan permintaan API menggunakan Fetch
        fetch('/api/cekkey?apikey=' + encodeURIComponent(apiKeyValue))
            .then(response => response.json())
            .then(data => {
                // Menampilkan Sweet Alert dengan hasil dari permintaan API
                if (data.status === true) {
                    const tableHtml = `
                        <table class="table">
                            <tbody>
                                <tr>
                                    <td><i class="fa fa-user"></i> Username</td>
                                    <td>${data.result.username}</td>
                                </tr>
                                <tr>
                                    <td><i class="fa fa-envelope"></i> Email</td>
                                    <td>${data.result.email}</td>
                                </tr>
                                <tr>
                                    <td><i class="fa fa-key"></i> Apikey</td>
                                    <td>${data.result.apikey}</td>
                                </tr>
                                    <td><i class="fa-solid fa-money-check"></i> Money</td>
                                    <td>${(data.result.money || 0)}</td>
                                </tr>
                                <tr>
                                    <td><i class="fa fa-key"></i> Limit</td>
                                    <td>${data.result.limit}</td>
                                </tr>
                                <tr>
                                    <td><i class="fa fa-star"></i> Premium</td>
                                    <td>${data.result.premium}</td>
                                </tr>
                                <tr>
                                    <td><i class="fa-solid fa-hourglass-end"></i> Expired</td>
                                    <td>${data.result.expired}</td>
                                </tr>
                                <tr>
                                    <td><i class="fa fa-clock"></i> Since</td>
                                    <td>${data.result.since}</td>
                                </tr>
                                <!-- Tambahkan baris ini untuk setiap field yang ingin ditampilkan -->
                            </tbody>
                        </table>
                    `;

                    Swal.fire({
                        html: tableHtml,
                        imageUrl: '<%= profile %>',
                        showCancelButton: false,
                        showConfirmButton: true,
                        allowOutsideClick: false,
                        allowEscapeKey: false,
                    });
                } else {
                    Swal.fire({
                        icon: 'error',
                        title: 'Error',
                        text: data.result,
                    });
                }
            })
            .catch(error => {
                console.error('Error:', error);
                // Menampilkan Sweet Alert jika terjadi kesalahan
                Swal.fire({
                    icon: 'error',
                    title: 'Error',
                    text: 'Terjadi kesalahan saat memproses permintaan API.',
                });
            });
    }
</script>
<script>
    // pc-component
    var elem = document.querySelectorAll('.header-user-profile a');
    for (var l = 0; l < elem.length; l++) {
        var pageUrl = window.location.href.split(/[?#]/)[0];
        if (elem[l].href == pageUrl && elem[l].getAttribute('href') != '') {
            elem[l].classList.add('active');
        }
    }

    // pencarian navbar
    document.querySelector('#compo-menu-search').addEventListener("keyup", function() {
        var tval = document.querySelector('#compo-menu-search').value.toLowerCase();
        var elem = document.querySelectorAll('.pc-item a');
        var resultsContainer = document.getElementById('search-results');
        resultsContainer.innerHTML = '';

        for (var l = 0; l < elem.length; l++) {
            var aval = elem[l].innerHTML.toLowerCase();
            var n = aval.indexOf(tval);

            if (n !== -1) {
                var resultItem = document.createElement('div');
                resultItem.classList.add('search-result-item');

                var resultLink = document.createElement('a');
                resultLink.href = elem[l].getAttribute('href');
                resultLink.textContent = elem[l].textContent;

                resultItem.appendChild(resultLink);
                resultsContainer.appendChild(resultItem);
            }
        }

        // Menambahkan style untuk menampilkan hasil pencarian
        resultsContainer.style.display = tval.length > 0 ? 'block' : 'none';
    });
</script>

<script>
    let apiKeyVisible = false;

    function toggleApiKeyVisibility() {
        const apiKeyIcon = document.getElementById('apiKeyIcon');
        const apiKeyDisplay = document.getElementById('apikey-display');
        const toggleButton = document.querySelector('.btn-primary');

        if (apiKeyVisible) {
            // Hide the API key
            Swal.fire({
                icon: 'success',
                title: 'API Key Hidden',
                text: 'API Key has been hidden.',
            }).then(() => {
                apiKeyDisplay.style.display = 'inline-block';
                apiKeyIcon.classList.remove('fa-eye-slash');
                apiKeyIcon.classList.add('fa-eye');
                apiKeyDisplay.textContent = '********';
            });
        } else {
            // Show the API key
            Swal.fire({
                icon: 'info',
                title: 'API Key Visible',
                text: 'API Key is now visible.',
            }).then(() => {
                apiKeyDisplay.style.display = 'inline-block';
                apiKeyIcon.classList.remove('fa-eye');
                apiKeyIcon.classList.add('fa-eye-slash');
                apiKeyDisplay.textContent = '<%= apikey %>';
            });
        }

        apiKeyVisible = !apiKeyVisible; // Toggle visibility
    }
</script>
