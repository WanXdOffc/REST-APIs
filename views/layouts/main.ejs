    <script>
        function updateResetTime() {
            // Fetch the current date and time
            const now = new Date();
    
            // Calculate the next reset time (in this case, daily at 23:00)
            const nextResetTime = new Date(now);
            nextResetTime.setHours(23, 0, 0, 0);
    
            // If the next reset time has already passed for today, set it for tomorrow
            if (now > nextResetTime) {
                nextResetTime.setDate(now.getDate() + 1);
            }
    
            // Calculate the time difference
            const timeDiff = nextResetTime - now;
            const hours = Math.floor(timeDiff / (1000 * 60 * 60));
            const minutes = Math.floor((timeDiff % (1000 * 60 * 60)) / (1000 * 60));
            const seconds = Math.floor((timeDiff % (1000 * 60)) / 1000);
    
            // Format the time difference
            const formattedTime = `${hours} Jam, ${minutes} Menit, ${seconds} Detik`;
    
            // Update the HTML content
            document.getElementById('reset-time').textContent = formattedTime;
        }
    
        // Call the function initially
        updateResetTime();
    
        // Set up an interval to update the reset time every second
        setInterval(updateResetTime, 1000);
    </script>
