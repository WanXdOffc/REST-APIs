<% if (success_msg !='' ) { %>
	<script>
		Swal.fire({
		title: 'Warning!',
		text: '<%- success_msg %>',
		icon: 'success',
		confirmButtonText: 'Oke'
		})
	</script>
<% } %>

<% if (warning_msg !='' ) { %>
	<script>
		Swal.fire({
		title: 'Warning!',
		text: '<%- warning_msg %>',
		icon: 'warning',
		confirmButtonText: 'Oke'
		})
	</script>
<% } %>

<% if (error_msg !='' ) { %>
	<script>
		Swal.fire({
		title: 'Error!',
